{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {"src": "../PotisanWmiLib", "files": ["**/*.csproj"]},
      ],
      "dest": "api",
      "filter": "filter.yml"
    }
  ],
  "build": {
    "content": [{"files": ["**/*.{md,yml}"], "exclude": ["_site/**"]}],
    "resource": [{"files": ["images/**"]}],
    "output": "_site",
    "template": ["default", "modern"],
    "globalMetadata": {
      "_appName": "PotisanWmiLib",
      "_appTitle": "PotisanWmiLib",
      "_enableSearch": true,
      "pdf": false
    }
  }
}